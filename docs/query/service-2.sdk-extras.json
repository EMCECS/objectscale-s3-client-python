{   
    "merge":{
      "operations":{
        "getmdkeys":{
          "name":"getmdkeys",
          "http":{
              "method":"GET",
              "requestUri":"/{Bucket}/?searchmetadata",
              "responseCode":200
          },
          "input":{"shape":"DisableMetadataSearchRequest"},
          "output":{"shape":"enableOutput"},
          "documentationUrl":"",
          "documentation":""
        },
        
        "disablemeta":{
            "name":"disablemeta",
            "http":{
                "method":"DELETE",
                "requestUri":"/{Bucket}?searchmetadata",
                "responseCode":204
            },
            "input":{"shape":"DisableMetadataSearchRequest"},
            "outuput":{"shape":"enableOutput"},
            "documentationUrl":"",
            "documentation":""
        },



        "MetadataSearch":{
          "name":"MetadataSearch",
          "http":{
            "method":"GET",
            "requestUri":"/?query={expression}",
            "responseCode":200
          },
          "input":{"shape":"QueryRequest"},
          "documentationUrl":"",
          "documentation":""
        }
    },
        "shapes":{
          "IndexableKeys":{
            "type":"list",
            "member":{
              "shape":"Key",
              "locationName":"Key"
          }
          },
          "Name":{
            "type":"string"
          },
          "Datatype":{
            "type":"string"
          },
          "AttributeName":{
            "type":"string"
          },
          "AttributeDatatype":{
            "type":"string"
          },
          "Key":{
            "type":"structure",
            "members":{
              "Name":{
                "shape":"Name"
              },
              "Datatype":{
                "shape":"Datatype"
              }
            }
          },
          "MetadataSearchList":{
            "type":"structure",
            "members":{
              "IndexableKeys":{
                "shape":"IndexableKeys"
              },
              "OptionalAttributes":{
                "shape":"OptionalAttributes"
              }
            }
          },
          "OptionalAttributes":{
            "type":"list",
            "member":{
              "shape":"Attribute"
            }
          },
          "Attribute":{
            "type":"structure",
            "members":{
              "Name":{
                "shape":"AttributeName"
              },
              "Datatype":{
                "shape":"AttributeDatatype"
              }
            }
          },
          "enableOutput":{
            "type":"structure",
            "members":{
              "IndexableKeys":{
                "shape":"IndexableKeys"
              },
              "OptionalAttributes":{
                "shape":"OptionalAttributes"
              }
            }
          
          },
          "DisableMetadataSearchRequest":{
            "type":"structure",
            "required":["Bucket"],
            "members":{
              "Bucket":{
                "shape":"BucketName",
                "contextParam":{"name":"Bucket"},
                "location":"uri",
                "locationName":"Bucket"
              }
            }
          },
            "MetaData":{
              "type":"string"
          },
          "CreateBucketRequest":{
              "members":{
                "MetaData":{
                  "shape":"MetaData",
                  "location":"header",
                  "locationName":"x-emc-metadata-search"
          }
        }
      },


      "QueryRequest":{
        "type":"structure",
        "required":["Query"],
        "members":{
          "Query":{
            "shape":"Query",
            "location":"uri",
            "locationName":"expression"
          }
        }
      },
      "Query":{
        "type":"string"
      }


    }
  }
}