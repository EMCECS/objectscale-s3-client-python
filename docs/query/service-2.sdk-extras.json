{   
    "merge":{
      "operations":{
        "getmdkeys":{
          "name":"getmdkeys",
          "http":{
              "method":"GET",
              "requestUri":"/{Bucket}/?searchmetadata",
              "responseCode":200
          },
          "input":{"shape":"DisableMetadataSearchRequest"},
          "output":{"shape":"MetadataSearchList"},
          "documentationUrl":"",
          "documentation":""
        },
        
        "disablemeta":{
            "name":"disablemeta",
            "http":{
                "method":"DELETE",
                "requestUri":"/{Bucket}?searchmetadata",
                "responseCode":204
            },
            "input":{"shape":"DisableMetadataSearchRequest"},
            "documentationUrl":"",
            "documentation":""
        },



        "MetadataSearch":{
          "name":"MetadataSearch",
          "http":{
            "method":"GET",
            "requestUri":"/?query={expression}",
            "responseCode":200
          },
          "input":{"shape":"QueryRequest"},
          "output":{"shape":"QueryResponse"},
          "documentationUrl":"",
          "documentation":""
        }
    },
        "shapes":{
          "IndexableKeys":{
            "type":"list",
            "member":{
              "shape":"Key",
              "locationName":"Key"
          }
          },
          "Name":{
            "type":"string"
          },
          "Datatype":{
            "type":"string"
          },
          "AttributeName":{
            "type":"string"
          },
          "AttributeDatatype":{
            "type":"string"
          },
          "Key":{
            "type":"structure",
            "members":{
              "Name":{
                "shape":"Name"
              },
              "Datatype":{
                "shape":"Datatype"
              }
            }
          },
          "MetadataSearchList":{
            "type":"structure",
            "members":{
              "MetadataSearchEnabled":{
                "shape":"MetadataSearchEnabled"
              },
              "IndexableKeys":{
                "shape":"IndexableKeys"
              }
            }
          },
          "MetadataSearchEnabled":{
            "type":"boolean"
          },
         
          "DisableMetadataSearchRequest":{
            "type":"structure",
            "required":["Bucket"],
            "members":{
              "Bucket":{
                "shape":"BucketName",
                "contextParam":{"name":"Bucket"},
                "location":"uri",
                "locationName":"Bucket"
              }
            }
          },
            "MetaData":{
              "type":"string"
          },
          "CreateBucketRequest":{
              "members":{
                "MetaData":{
                  "shape":"MetaData",
                  "location":"header",
                  "locationName":"x-emc-metadata-search"
          }
        }
      },


      "QueryRequest":{
        "type":"structure",
        "required":["Query"],
        "members":{
          "Query":{
            "shape":"Query",
            "location":"uri",
            "locationName":"expression"
          }
        }
      },
      "Query":{
        "type":"string"
      },
      
      "QueryResponse":{
        "type":"structure",
        "members":{
          "Buckets":{
            "shape":"Buckets"
          },
          "IsTruncated":{
            "shape":"IsTruncated"
          }
        }
      },
      "Buckets":{
        "type":"list",
        "member":{
          "Bucket":{
            "shape":"Bucket"
          }
        }
      }

    }
  }
}