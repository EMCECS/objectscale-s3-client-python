{   
    "merge":{
      "operations":{
        "getmdkeys":{
          "name":"getmdkeys",
          "http":{
              "method":"GET",
              "requestUri":"/{Bucket}/?searchmetadata",
              "responseCode":200
          },
          "input":{"shape":"DisableMetadataSearchRequest"},
          "output":{"shape":"MetadataSearchList"},
          "documentationUrl":"",
          "documentation":""
        },
        
        "disablemeta":{
            "name":"disablemeta",
            "http":{
                "method":"DELETE",
                "requestUri":"/{Bucket}?searchmetadata",
                "responseCode":204
            },
            "input":{"shape":"DisableMetadataSearchRequest"},
            "documentationUrl":"",
            "documentation":""
        },

        
        "MetadataSearch":{
          "name":"MetadataSearch",
          "http":{
            "method":"GET",
            "requestUri":"/{Bucket}/?query={expression}",
            "responseCode":200
          },
          "input":{"shape":"QueryRequest"},
          "output":{"shape":"QueryResponse"},
          "documentationUrl":"",
          "documentation":""
        }
    },
        "shapes":{
          "IndexableKeys":{
            "type":"list",
            "member":{
              "shape":"Key",
              "locationName":"Key"
          }
          },
          "Name":{
            "type":"string"
          },
          "Datatype":{
            "type":"string"
          },
          "AttributeName":{
            "type":"string"
          },
          "AttributeDatatype":{
            "type":"string"
          },
          "Key":{
            "type":"structure",
            "members":{
              "Name":{
                "shape":"Name"
              },
              "Datatype":{
                "shape":"Datatype"
              }
            }
          },
          "MetadataSearchList":{
            "type":"structure",
            "members":{
              "MetadataSearchEnabled":{
                "shape":"MetadataSearchEnabled"
              },
              "IndexableKeys":{
                "shape":"IndexableKeys"
              }
            }
          },
          "MetadataSearchEnabled":{
            "type":"boolean"
          },
         
          "DisableMetadataSearchRequest":{
            "type":"structure",
            "required":["Bucket"],
            "members":{
              "Bucket":{
                "shape":"BucketName",
                "contextParam":{"name":"Bucket"},
                "location":"uri",
                "locationName":"Bucket"
              }
            }
          },
            "MetaData":{
              "type":"string"
          },
          "CreateBucketRequest":{
              "members":{
                "MetaData":{
                  "shape":"MetaData",
                  "location":"header",
                  "locationName":"x-emc-metadata-search"
          }
        }
      },


      "QueryRequest":{
        "type":"structure",
        "required":["Bucket","Query"],
        "members":{
          "Bucket":{
            "shape":"BucketName",
            "location":"uri",
            "locationName":"Bucket"
          },
          "Query":{
            "shape":"Query",
            "location":"uri",
            "locationName":"expression"
          },
          "Attributes":{
            "shape":"Attributes",
            "location":"querystring",
            "locationName":"attributes"
          },
          "Sorted":{
            "shape":"Sorted",
            "location":"querystring",
            "locationName":"sorted"
          },
          "IncludeOlderVersion":{
            "shape":"IncludeOlderVersions",
            "location":"querystring",
            "locationName":"include_older_version"
          },
          "MaxKeys":{
            "shape":"MaxKeys",
            "location":"querystring",
            "locationName":"max-keys"
          },
          "Marker":{
            "shape":"Marker",
            "location":"querystring",
            "locationName":"marker"
          }
        }
      },
      "Query":{"type":"string"},
      "Attributes":{"type":"string"},
      "Sorted":{"type":"string"},
      "IncludeOlderVersions":{"type":"boolean"},
      "MaxKeys":{"type":"integer"},
      "Marker":{"type":"string"},
      
      "QueryResponse":{
        "type":"structure",
        "members":{
          "Name":{
            "shape":"BucketName"
          },
          "NextMarker":{
            "shape":"NextMarker"
          },
          "IsTruncated":{
            "shape":"IsTruncated"
          },
          "MaxKeys":{
            "shape":"MaxKeys"
          },
          "ObjectMatches":{
            "shape":"ObjectMatches"
          },
          "CommonPrefixMatches":{
            "shape":"CommonPrefixMatches"
          }
        }
      },
      "NextMarker":{"type":"string"},
      "ObjectMatches":{
        "type":"list",
        "member":{
          "shape":"object"
        }
      },
      "object":{
        "type":"structure",
        "members":{
          "objectName":{"shape":"objectName"},
          "objectId":{"shape":"objectId"},
          "versionId":{"shape":"versionId"},
          "queryMds":{"shape":"queryMds"},
          "queryBinaryMds":{"shape":"queryBinaryMds"},
          "indexKey":{"shape":"indexKey"},
          "isFanoutObject":{"shape":"isFanoutObject"}
        }
      },
      "objectName":{"type":"string"},
      "objectId":{"type":"string"},
      "versionId":{"type":"string"},
      "objectOwnerZone":{"type":"string"},
      "queryMds":{
        "type":"structure",
        "members":{
          "type":{"shape":"MDtype"},
          "mdMap":{"shape":"Metadata"}
        }
      },
      "MDtype":{"type":"string"},
      "queryBinaryMds":{
        "type":"structure",
        "members":{
          "left":{"shape":"left"},
          "right":{"shape":"right"}
        }
      },
      "left":{"type":"string"},
      "right":{"type":"string"},
      "indexKey":{"type":"string"},
      "isFanoutObject":{"type":"boolean"},
      "CommonPrefixMatches":{
        "type":"structure",
        "members":{
          "PrefixGroups":{"shape":"PrefixGroups"}
        }
      },
      "PrefixGroups":{"shape":"string"}

        
      

    }
  }
}